

BUILDROOT = $$absolute_path($$PWD)/bin
DESTDIR = $$BUILDROOT/tests/benchmarks

CONFIG(debug, debug|release) {
    MOC_DIR = $$BUILDROOT/debug/tests/benchmarks
    OBJECTS_DIR = $$BUILDROOT/debug/tests/benchmarks

} else: CONFIG(release, debug|release) {
    MOC_DIR = $$BUILDROOT/release/tests/benchmarks
    OBJECTS_DIR = $$BUILDROOT/release/tests/benchmarks
}
